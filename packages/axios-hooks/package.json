{
  "name": "@motech-development/axios-hooks",
  "version": "0.0.0",
  "description": "Axios hooks for React",
  "main": "lib/axios-hooks.cjs.js",
  "module": "lib/axios-hooks.esm.js",
  "types": "lib/axios-hooks.d.ts",
  "scripts": {
    "build": "run prebuild && NODE_ENV=production rollup -c && run postbuild",
    "lint": "eslint -c .eslintrc.yml './src/**/*.{ts,tsx}'",
    "package": "run build",
    "postbuild": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "prebuild": "rimraf ./lib",
    "test": "jest --watch",
    "test-ci": "jest --coverage --runInBand"
  },
  "author": "Mo Gusbi <mo.gusbi@motechdevelopment.co.uk> (https://motechdevelopment.co.uk)",
  "license": "ISC",
  "dependencies": {
    "axios": "0.27.2",
    "react": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.50",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.28.0",
    "jest": "26.6.0",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.22",
    "typescript": "4.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{ts,tsx}": "eslint -c .eslintrc.yml --fix"
  }
}
