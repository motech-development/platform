{
  "name": "@motech-development/axios-hooks",
  "version": "0.0.0",
  "description": "Axios hooks for React",
  "main": "lib/axios-hooks.cjs.js",
  "module": "lib/axios-hooks.esm.js",
  "types": "lib/axios-hooks.d.ts",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint -c .eslintrc.yml './src/**/*.{ts,tsx}'",
    "prebuild": "rimraf ./lib",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prettier": "prettier --config ../../.prettierrc.yml --check 'src/**/*'",
    "test": "jest --watch",
    "test:ci": "jest --coverage --runInBand --reporters=default --reporters=jest-junit"
  },
  "author": "Mo Gusbi <mo.gusbi@motechdevelopment.co.uk> (https://motechdevelopment.co.uk)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "axios": "0.19.2",
    "react": "16.13.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleDirectories": [
      "../../node_modules",
      "<rootDir>/node_modules"
    ]
  },
  "jest-junit": {
    "outputDirectory": "../../reports/junit",
    "outputName": "axios-hooks.xml"
  }
}
