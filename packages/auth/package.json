{
  "name": "@motech-development/auth",
  "version": "0.0.0",
  "description": "Breeze UI authentication library",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint": "eslint -c .eslintrc.yml './src/**/*.{ts,tsx}'",
    "package": "yarn build",
    "postbuild": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "prebuild": "rimraf ./lib",
    "precommit": "lint-staged",
    "prettier": "prettier --config ../../.prettierrc.yml --write 'src/**/*'",
    "test": "jest --watch",
    "test:ci": "jest --coverage --runInBand"
  },
  "author": "Mo Gusbi <mo.gusbi@motechdevelopment.co.uk> (https://motechdevelopment.co.uk)",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.14.0",
    "@motech-development/query-string-hook": "^0.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.4",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "jest": "26.6.0",
    "lint-staged": "12.3.7",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.63.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.22",
    "typescript": "4.2.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  }
}
