    "declaration": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noFallthroughCasesInSwitch": true,
    "outDir": "lib",
    "target": "ESNext"
  },
  "exclude": ["lib"],
  "extends": "../../tsconfig.json",
  "include": ["**/*.ts", "**/*.tsx"]
}
