{
  "name": "@motech-development/id",
  "version": "0.0.0",
  "description": "Motech Development universal login",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "deploy:infrastructure": "serverless deploy --verbose",
    "deploy": "serverless client deploy --verbose --no-confirm",
    "postdeploy": "a0deploy import -c config.json -i tenant.yaml",
    "lint": "eslint -c .eslintrc.yml './src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prettier": "prettier --config ../../.prettierrc.yml --check 'src/**/*'",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "teardown": "serverless client remove --verbose --no-confirm",
    "teardown:infrastructure": "serverless remove --verbose"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@motech-development/breeze-ui": "^0.0.0",
    "auth0-js": "^9.12.1",
    "formik": "^2.0.8",
    "history": "^4.10.1",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-popper": "^1.3.7",
    "styled-components": "^4.4.1",
    "styled-reset": "^4.0.8",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.10.7",
    "@types/yup": "^0.26.27",
    "auth0-deploy-cli": "^3.6.6",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.18.12",
    "gatsby-cli": "^2.8.22",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-transformer-sharp": "^2.3.7"
  }
}
