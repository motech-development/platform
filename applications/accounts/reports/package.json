{
  "name": "@accounts/reports",
  "version": "0.0.0",
  "description": "Motech Development accounts report generator service",
  "private": true,
  "scripts": {
    "deploy": "serverless deploy --verbose",
    "lint": "eslint -c .eslintrc.yml './src/**/*.ts'",
    "precommit": "lint-staged",
    "prettier": "prettier --config ../../../.prettierrc.yml --write 'src/**/*'",
    "teardown": "serverless remove --verbose",
    "test": "jest --watch",
    "test:ci": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@types/aws4": "1.5.1",
    "@motech-development/s3-file-operations": "^0.0.0",
    "archiver": "5.3.1",
    "aws4": "1.11.0",
    "axios": "0.21.2",
    "json-2-csv": "3.16.1",
    "luxon": "2.3.0",
    "qs": "6.10.5",
    "uuid": "8.3.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@types/archiver": "5.3.1",
    "@types/aws-lambda": "8.10.101",
    "@types/jest": "26.0.24",
    "@types/luxon": "2.0.9",
    "@types/qs": "6.9.7",
    "@types/uuid": "8.3.4",
    "@types/yup": "0.29.11",
    "aws-lambda-mock-context": "3.2.1",
    "babel-loader": "8.1.0",
    "cache-loader": "4.1.0",
    "fork-ts-checker-webpack-plugin": "6.5.2",
    "jest": "26.6.0",
    "jest-date-mock": "1.0.8",
    "serverless": "2.70.0",
    "serverless-deployment-bucket": "1.5.2",
    "serverless-prune-plugin": "2.0.1",
    "serverless-step-functions": "3.5.1",
    "serverless-webpack": "5.6.1",
    "webpack": "4.44.2",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "jest-date-mock"
    ],
    "testEnvironment": "node"
  }
}
