{
  "name": "@accounts/reports",
  "version": "0.0.0-development",
  "description": "Motech Development accounts report generator service",
  "scripts": {
    "deploy": "serverless deploy --verbose",
    "lint": "eslint --cache './src/**/*.ts'",
    "teardown": "serverless remove --verbose",
    "test": "jest --watch",
    "test-ci": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@motech-development/s3-file-operations": "workspace:*",
    "archiver": "5.3.1",
    "aws-sdk": "2.1241.0",
    "aws4-axios": "2.4.9",
    "axios": "0.27.2",
    "json-2-csv": "3.17.2",
    "luxon": "2.3.0",
    "qs": "6.11.0",
    "uuid": "8.3.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@motech-development/eslint-config-motech-base": "workspace:*",
    "@motech-development/prettier-motech-config": "workspace:*",
    "@motech-development/semantic-release": "workspace:*",
    "@types/archiver": "5.3.1",
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "27.5.2",
    "@types/luxon": "2.0.9",
    "@types/qs": "6.9.7",
    "@types/uuid": "8.3.4",
    "@types/yup": "0.29.11",
    "aws-lambda-mock-context": "3.2.1",
    "babel-loader": "9.1.0",
    "eslint": "8.33.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "27.5.1",
    "jest-date-mock": "1.0.8",
    "prettier": "2.3.0",
    "serverless": "3.27.0",
    "serverless-deployment-bucket": "1.6.0",
    "serverless-prune-plugin": "2.0.1",
    "serverless-step-functions": "3.11.0",
    "serverless-webpack": "^5.11.0",
    "typescript": "4.2.3",
    "webpack": "5.75.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "jest-date-mock"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{ts,tsx}": "eslint --cache --fix"
  }
}
