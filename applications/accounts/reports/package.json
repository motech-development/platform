{
  "name": "@accounts/reports",
  "version": "0.0.0",
  "description": "Motech Development accounts report generator service",
  "private": true,
  "scripts": {
    "deploy": "yarn run -T serverless deploy --verbose",
    "lint": "yarn run -T eslint -c .eslintrc.yml './src/**/*.ts'",
    "precommit": "yarn run -T lint-staged",
    "prettier": "yarn run -T prettier --config ../../../.prettierrc.yml --write 'src/**/*'",
    "teardown": "yarn run -T serverless remove --verbose",
    "test": "yarn run -T jest --watch",
    "test-ci": "yarn run -T jest --coverage --runInBand"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "jest-date-mock"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@motech-development/s3-file-operations": "^0.0.0",
    "archiver": "^5.3.0",
    "aws4": "^1.11.0",
    "axios": "^0.21.1",
    "json-2-csv": "^3.10.3",
    "luxon": "^2.0.2",
    "qs": "^6.10.1",
    "uuid": "^8.3.2",
    "yup": "^0.29.1"
  }
}
