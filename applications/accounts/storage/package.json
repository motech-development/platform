    "test-ci": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@motech-development/api-gateway-handler": "workspace:packages/api-gateway-handler",
    "@motech-development/s3-file-operations": "workspace:packages/s3-file-operations",
    "aws-sdk": "2.1241.0",
    "uuid": "8.3.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "26.0.24",
    "@types/uuid": "8.3.4",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "aws-lambda-mock-context": "3.2.1",
    "babel-loader": "8.1.0",
    "cache-loader": "4.1.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "6.5.2",
    "jest": "26.6.0",
    "prettier": "2.3.0",
    "serverless": "2.72.3",
    "serverless-deployment-bucket": "1.5.2",
    "serverless-offline": "8.8.1",
    "serverless-prune-plugin": "2.0.1",
    "serverless-webpack": "serverless-heaven/serverless-webpack#1a6c72ffbbbaba5cd06bcdd27d0bca948cc1fc15",
    "typescript": "4.2.3",
    "webpack": "4.46.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
