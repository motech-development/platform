{
  "name": "@accounts/storage",
  "version": "0.0.0",
  "description": "Motech Development accounts file storage",
  "private": true,
  "scripts": {
    "deploy": "serverless deploy --verbose",
    "lint": "eslint -c .eslintrc.yml './src/**/*.ts'",
    "teardown": "serverless remove --verbose",
    "test": "jest --watch",
    "test-ci": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@motech-development/api-gateway-handler": "workspace:packages/api-gateway-handler",
    "@motech-development/s3-file-operations": "workspace:packages/s3-file-operations",
    "aws-sdk": "2.1046.0",
    "uuid": "8.3.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@types/aws-lambda": "8.10.101",
    "@types/jest": "26.0.24",
    "@types/uuid": "8.3.4",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "aws-lambda-mock-context": "3.2.1",
    "babel-loader": "8.1.0",
    "cache-loader": "4.1.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "fork-ts-checker-webpack-plugin": "6.5.2",
    "jest": "26.6.0",
    "lint-staged": "12.3.7",
    "prettier": "2.3.0",
    "serverless": "2.70.0",
    "serverless-deployment-bucket": "1.5.2",
    "serverless-offline": "8.3.1",
    "serverless-prune-plugin": "2.0.1",
    "serverless-webpack": "5.10.0",
    "typescript": "4.2.3",
    "webpack": "4.44.2",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{ts,tsx}": "eslint -c .eslintrc.yml --fix"
  }
}
