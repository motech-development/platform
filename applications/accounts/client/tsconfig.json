{
  "compilerOptions": {
    "allowJs": false,
    "downlevelIteration": true,
    "esModuleInterop": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "target": "ESNext",
    "useDefineForClassFields": true
  },
  "extends": "../../../tsconfig.json",
  "include": ["src"],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
