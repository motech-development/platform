{
  "name": "@accounts/client",
  "version": "0.0.0",
  "description": "Motech Development accounts client application",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-icons": "pwa-asset-generator ./assets/icon.svg ./public --icon-only --favicon --background '#161616' --type png --padding '20%' --maskable false",
    "build-splash:landscape": "pwa-asset-generator ./assets/splash.svg ./public --splash-only --background '#161616' --type png --padding 'calc(50vh - 5%) calc(50vw - 10%)' --landscape-only",
    "build-splash:portrait": "pwa-asset-generator ./assets/splash.svg ./public --splash-only --background '#161616' --type png --portrait-only",
    "deploy": "serverless client deploy --verbose --no-confirm",
    "e2e": "cypress open",
    "e2e-ci": "cy2 run --browser chrome --record --parallel --headless",
    "lint": "eslint -c .eslintrc.yml './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "teardown": "serverless client remove --verbose --no-confirm",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --coverage --runInBand"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@motech-development/appsync-apollo": "workspace:packages/appsync-apollo",
    "@motech-development/auth": "workspace:packages/auth",
    "@motech-development/axios-hooks": "workspace:packages/axios-hooks",
    "@motech-development/breeze-ui": "workspace:packages/breeze-ui",
    "@motech-development/ga-web-vitals": "workspace:packages/ga-web-vitals",
    "@motech-development/query-string-hook": "workspace:packages/query-string-hook",
    "axios": "0.27.2",
    "decimal.js": "10.3.1",
    "file-saver": "2.0.5",
    "formik": "2.2.6",
    "graphql": "16.2.0",
    "history": "4.10.1",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.8",
    "i18next-http-backend": "1.3.2",
    "luxon": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-ga": "3.3.1",
    "react-i18next": "11.15.4",
    "react-idle-timer": "4.6.4",
    "react-router-dom": "5.3.4",
    "styled-components": "5.3.0",
    "web-vitals": "2.1.4",
    "workbox-background-sync": "6.4.2",
    "workbox-broadcast-update": "6.4.2",
    "workbox-cacheable-response": "6.4.2",
    "workbox-core": "6.4.2",
    "workbox-expiration": "6.4.2",
    "workbox-google-analytics": "6.4.2",
    "workbox-navigation-preload": "6.4.2",
    "workbox-precaching": "6.4.2",
    "workbox-range-requests": "6.4.2",
    "workbox-routing": "6.4.2",
    "workbox-strategies": "6.4.2",
    "workbox-streams": "6.4.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.3",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "13.5.0",
    "@types/file-saver": "2.0.5",
    "@types/jest": "26.0.24",
    "@types/luxon": "2.0.9",
    "@types/react": "17.0.50",
    "@types/react-dom": "17.0.17",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.9",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "axe-core": "4.4.3",
    "cy2": "2.1.0",
    "cypress": "10.10.0",
    "cypress-axe": "0.14.0",
    "cypress-file-upload": "5.0.8",
    "cypress-localstorage-commands": "1.7.0",
    "cypress-log-to-output": "1.1.2",
    "cypress-pipe": "2.0.0",
    "cypress-wait-until": "1.7.2",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-date-mock": "1.0.8",
    "jest-styled-components": "7.1.1",
    "prettier": "2.3.0",
    "pwa-asset-generator": "6.2.0",
    "react-scripts": "5.0.1",
    "serverless": "2.72.3",
    "serverless-deployment-bucket": "1.5.2",
    "serverless-finch": "2.8.0",
    "typescript": "4.2.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/{history,index,i18n}.{ts,tsx}",
      "!src/**/*/index.{ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(es-cookie))"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{ts,tsx}": "eslint -c .eslintrc.yml --fix"
  }
}
