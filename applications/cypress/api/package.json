{
  "name": "@cypress/api",
  "version": "0.0.0",
  "description": "Cypress API",
  "private": true,
  "scripts": {
    "deploy": "serverless deploy --verbose",
    "lint": "eslint -c .eslintrc.yml './src/**/*.ts'",
    "precommit": "lint-staged",
    "prettier": "prettier --config ../../../.prettierrc.yml --write 'src/**/*'",
    "teardown": "serverless remove --verbose",
    "test": "jest --watch",
    "test:ci": "jest --coverage --runInBand"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@motech-development/api-gateway-handler": "^0.0.0",
    "@motech-development/logger": "^0.0.0",
    "@octokit/auth-app": "^3.4.1",
    "@octokit/rest": "^18.5.6",
    "luxon": "^1.26.0",
    "yup": "^0.29.1"
  }
}
