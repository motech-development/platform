{
  "name": "@cypress/api",
  "version": "0.0.0-development",
  "description": "Cypress API",
  "scripts": {
    "deploy": "serverless deploy --verbose",
    "lint": "eslint --cache './src/**/*.ts'",
    "teardown": "serverless remove --verbose",
    "test": "jest --watch",
    "test-ci": "jest --coverage --runInBand"
  },
  "dependencies": {
    "@motech-development/api-gateway-handler": "workspace:*",
    "@motech-development/node-logger": "workspace:*",
    "@octokit/auth-app": "3.6.1",
    "@octokit/rest": "18.12.0",
    "aws-sdk": "2.1241.0",
    "luxon": "2.5.2",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@motech-development/eslint-config-motech-base": "workspace:*",
    "@motech-development/prettier-motech-config": "workspace:*",
    "@motech-development/semantic-release": "workspace:*",
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "27.5.2",
    "@types/luxon": "2.0.9",
    "@types/yup": "0.29.11",
    "aws-lambda-mock-context": "3.2.1",
    "babel-loader": "9.1.0",
    "eslint": "8.33.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "jest": "27.5.1",
    "prettier": "2.3.0",
    "serverless": "3.27.0",
    "serverless-deployment-bucket": "1.6.0",
    "serverless-prune-plugin": "2.0.1",
    "serverless-webpack": "^5.11.0",
    "typescript": "4.2.3",
    "webpack": "5.75.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{ts,tsx}": "eslint --cache --fix"
  }
}
