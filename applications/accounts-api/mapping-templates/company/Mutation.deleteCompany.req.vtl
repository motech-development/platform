{
  "version": "2017-02-28",
  "operation": "DeleteItem",
  "key" : {
    "pk": $util.dynamodb.toDynamoDBJson($ctx.args.pk),
    "sk": $util.dynamodb.toDynamoDBJson("COMPANY")
  },
  "condition": {
    "expression": "#owner = :owner",
    "expressionNames": {
      "#owner": "owner"
    },
    "expressionValues": {
      ":owner": $util.dynamodb.toDynamoDBJson($ctx.identity.sub)
    }
  }
}
